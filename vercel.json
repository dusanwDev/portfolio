{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "ng build",
  "outputDirectory": "dist/portfolio",
  "devCommand": "ng serve --port $PORT",
  "installCommand": "npm install",
  "framework": "angular",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
